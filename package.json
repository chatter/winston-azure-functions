{
  "name": "winston-azure-functions",
  "version": "0.0.2",
  "description": "Winston transport for azure functions",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "jest",
    "precommit": "lint-staged",
    "prepare": "taskr",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --no-semi --single-quote --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/upcompass/winston-azure-functions.git"
  },
  "keywords": [
    "winston",
    "logging",
    "azure",
    "functions"
  ],
  "author": "oss@e107.com.ar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/upcompass/winston-azure-functions/issues"
  },
  "homepage": "https://github.com/upcompass/winston-azure-functions#readme",
  "dependencies": {
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@taskr/clear": "^1.1.0",
    "@taskr/esnext": "^1.1.0",
    "@taskr/shell": "^1.1.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.14.3",
    "jest": "^21.0.1",
    "lint-staged": "^4.1.0",
    "prettier": "^1.6.1",
    "semantic-release": "^8.0.3",
    "taskr": "^1.1.0",
    "ts-jest": "^21.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "typescript": "^2.5.2",
    "validate-commit-msg": "^2.14.0"
  }
}
